{
  "version": "0.2.0",
  "configurations": [
        {
           "name": "Attach to Chrome, with sourcemaps",
            "type": "chrome",
            "request": "attach",
            "url": "http://localhost:4200/home/home",
            "port": 9222,
            "sourceMaps": true,
            "trace": true,
            "webRoot": "${workspaceRoot}"     
        }
  ]
}
